{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "build/**",
                ".next/**",
                "!.next/cache/**"
            ]
        },
        "start": {
            "dependsOn": [
                "^start"
            ]
        },
        "dev": {
            "cache": false
        },
        "dev:hot": {
            "cache": false
        },
        "lint": {
            "cache": false
        },
        "lint:fix": {
            "cache": false
        }
    }
}